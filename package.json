{
  "name": "decentraland",
  "version": "0.6.3",
  "description": "CLI tool for parcel management.",
  "main": "./dist/index.js",
  "scripts": {
    "start": "(mkdir tmp > /dev/null 2>&1 || true) && cd tmp && NODE_ENV=dev ../bin/dcl",
    "watch": "tsc --watch & dcl-scripting build.json --watch",
    "build": "npm run clean && tsc && npm run linker:build && npm run copy:samples",
    "copy:samples": "cp -r src/samples dist/samples",
    "clean": "rimraf dist && rimraf tmp",
    "linker:dev": "next",
    "linker:start": "npm run linker:build && ts-node src/index.ts link",
    "linker:build": "dcl-scripting build.json",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "tsc": "tsc",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decentraland/cli.git"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 140
  },
  "keywords": [
    "decentraland",
    "cli"
  ],
  "author": "Decentraland",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/decentraland/cli/issues"
  },
  "homepage": "https://github.com/decentraland/cli#readme",
  "bin": {
    "dcl": "./bin/dcl"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@types/analytics-node": "0.0.31",
    "@types/uuid": "^3.4.3",
    "analytics-node": "^3.2.0",
    "axios": "^0.17.1",
    "chalk": "^2.3.1",
    "dcl-sdk": "^2.2.7",
    "decentraland-commons": "^3.1.1",
    "decentraland-eth": "^1.1.3",
    "docker-names": "^1.0.3",
    "express": "^4.16.2",
    "fs-extra": "^5.0.0",
    "get-installed-path": "^4.0.8",
    "gitignore-parser": "0.0.2",
    "inquirer": "^5.1.0",
    "ipfs-api": "^17.5.0",
    "opn": "^5.2.0",
    "progress": "^2.0.0",
    "root-check": "^1.0.0",
    "uuid": "^3.2.1",
    "vorpal": "^1.12.0",
    "wildcards": "^1.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^5.0.0",
    "@types/inquirer": "0.0.36",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.5.2",
    "@types/opn": "^5.1.0",
    "@types/progress": "^2.0.1",
    "@types/react-dom": "^16.0.4",
    "@types/sinon": "^4.3.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.1.3",
    "mocha": "^4.1.0",
    "prettier": "^1.9.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "rimraf": "^2.6.2",
    "sinon": "^4.5.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.1.6"
  }
}
