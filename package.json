{
  "name": "decentraland",
  "version": "3.8.1",
  "description": "Decentraland CLI developer tool.",
  "bin": {
    "dcl": "dist/cli.js"
  },
  "files": [
    "bin",
    "dist",
    "samples",
    "abi"
  ],
  "scripts": {
    "start": "npm run watch",
    "watch": "tsc -p tsconfig.json -w",
    "build": "tsc -p tsconfig.json",
    "lint": "tslint -e 'samples/**/*' -e '*.json' -c tslint.json 'src/**/*.ts'",
    "test": "FORCE_COLOR=1 ava",
    "test:dry": "FORCE_COLOR=1 ava --update-snapshots",
    "test:ci": "FORCE_COLOR=1 ava -v --fail-fast --match='!*E2E*' --match='!*Decentraland.deploy()*'",
    "test:win": "set FORCE_COLOR=1 ava -v --fail-fast --match=\"!*E2E*\" --match=\"!*Decentraland.deploy()*\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "*.ts": [
        "tslint -c tslint.json",
        "prettier --single-quote --no-semi --write",
        "git add"
      ]
    },
    "ignore": [
      "samples/**/*.ts"
    ]
  },
  "repository": "decentraland/cli",
  "keywords": [
    "decentraland",
    "cli",
    "dcl",
    "mana",
    "land"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/decentraland/cli",
  "devDependencies": {
    "@dcl/schemas": "^1.8.0",
    "@types/analytics-node": "0.0.32",
    "@types/archiver": "^3.0.0",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^5.0.0",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "0.0.36",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/node": "^12.12.55",
    "@types/puppeteer": "^1.11.2",
    "@types/request": "^2.48.1",
    "@types/semver": "^7.3.9",
    "@types/sinon": "^5.0.5",
    "@types/ws": "^6.0.1",
    "@walletconnect/core": "^1.0.0-beta.37",
    "@walletconnect/types": "^1.0.0-beta.37",
    "@walletconnect/utils": "^1.0.0-beta.37",
    "analytics-node": "^3.2.0",
    "archiver": "^3.1.1",
    "arg": "^4.1.0",
    "ava": "^1.2.1",
    "chalk": "^2.3.1",
    "chokidar": "^2.0.3",
    "cors": "^2.8.4",
    "cross-spawn": "^6.0.5",
    "dcl-catalyst-client": "^5.0.3",
    "dcl-tslint-config-standard": "^1.0.1",
    "decentraland-dapps": "^8.1.0",
    "docker-names": "^1.0.3",
    "eth-connect": "^0.3.0",
    "ethers": "^4.0.38",
    "express": "^4.16.2",
    "fs-extra": "^7.0.1",
    "git-rev-sync": "^1.11.1",
    "glob": "^7.1.3",
    "google-protobuf": "^3.8.0",
    "husky": "^1.1.3",
    "ignore": "^4.0.6",
    "inquirer": "^6.2.0",
    "isomorphic-fetch": "^2.2.1",
    "lint-staged": "^8.0.5",
    "mkdirp": "^0.5.1",
    "opn": "^5.2.0",
    "ora": "^3.0.0",
    "package-json": "^7.0.0",
    "portfinder": "^1.0.13",
    "prettier": "^1.14.0",
    "puppeteer": "^1.17.0",
    "request": "^2.88.0",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "sinon": "^7.1.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.11.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^4.4.3",
    "uuid": "^3.3.2",
    "wildcards": "^1.0.2",
    "ws": "^6.0.0"
  },
  "dependencies": {
    "@dcl/linker-dapp": "^0.1.3",
    "dcl-catalyst-commons": "^3.0.0",
    "dcl-node-runtime": "^1.0.0",
