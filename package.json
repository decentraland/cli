{
  "name": "dcl-cli",
  "version": "0.1.4",
  "description": "CLI tool for parcel management.",
  "main": "./dist/index.js",
  "scripts": {
    "start": "./dev.js",
    "build": "npm run clean && tsc && npm run linker:build",
    "clean": "rimraf dist && rimraf tmp",
    "linker:dev": "next",
    "linker:start": "npm run linker:build && ts-node src/index.ts link",
    "linker:build": "next build && next export -o dist/linker-app",
    "copy:template": "copyfiles -u 1 template/* template/**/* dist/template",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "tsc": "tsc",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decentraland/cli.git"
  },
  "keywords": [
    "decentraland",
    "cli"
  ],
  "author": "Decentraland",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/decentraland/cli/issues"
  },
  "homepage": "https://github.com/decentraland/cli#readme",
  "bin": {
    "dcl": "./bin/index.js"
  },
  "dependencies": {
    "@types/fs-extra": "^5.0.0",
    "@types/next": "^2.4.7",
    "babel-polyfill": "^6.26.0",
    "budo": "^10.0.4",
    "chalk": "^2.3.0",
    "decentraland-commons": "git+https://github.com/decentraland/commons.git#release",
    "decentraland-contracts": "git+https://github.com/decentraland/contracts.git#release",
    "fs-extra": "^5.0.0",
    "get-installed-path": "^4.0.8",
    "inquirer": "^5.0.0",
    "ipfs-api": "^17.2.6",
    "koa": "^2.4.1",
    "koa-router": "^7.3.0",
    "koa-static": "^4.0.2",
    "next": "^4.2.2",
    "progress": "^2.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "root-check": "^1.0.0",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "0.0.36",
    "@types/koa": "^2.0.43",
    "@types/koa-router": "^7.0.27",
    "@types/koa-static": "^4.0.0",
    "@types/mocha": "^2.2.45",
    "@types/node": "^8.5.2",
    "@types/progress": "^2.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.1.3",
    "mocha": "^4.1.0",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.6"
  }
}
